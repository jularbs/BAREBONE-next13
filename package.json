{
  "name": "kbp-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "next start -p $PORT",
    "build": "next build",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && npm install && npm run dev",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js asset/scss/nextjs-argon-dashboard-pro.scss asset/css/nextjs-argon-dashboard-pro.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js asset/scss/nextjs-argon-dashboard-pro.scss asset/css/nextjs-argon-dashboard-pro.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js asset/scss/nextjs-argon-dashboard-pro.scss asset/css/nextjs-argon-dashboard-pro.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss",
    "dev": "next dev -- -p 3002"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "5.5.0",
    "@tinymce/tinymce-react": "^3.14.0",
    "axios": "^0.22.0",
    "bootstrap": "4.6.0",
    "classnames": "2.2.6",
    "date-fns": "^2.29.3",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^13.0.0",
    "next-images": "^1.8.4",
    "nprogress": "^0.2.0",
    "prop-types": "15.7.2",
    "react": "^18.2.0",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.3.1",
    "react-notification-alert": "0.0.13",
    "react-odometerjs": "^2.1.2",
    "react-pdf": "^5.7.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-player": "^2.9.0",
    "react-visibility-sensor": "^5.1.1",
    "reactstrap": "8.9.0",
    "sass": "^1.55.0"
  },
  "optionalDependencies": {
    "@fortawesome/fontawesome-free": "5.15.2",
    "quill": "1.3.7",
    "select2": "4.1.0-rc.0",
    "sweetalert2": "10.14.0"
  },
  "engines": {
    "node": "14.21.3",
    "npm": "6.14.18"
  }
}
